<title>Simple Map</title>
<style>
  /* Always set the map height explicitly to define the size of the div
               * element that contains the map. */
  #map {
    height: 90vh;
  }


</style>
<div id="map"></div>
<script>
  var map;
  function initMap() {
    map = new google.maps.Map(document.getElementById('map'), {
      center: { lat: 47.6614, lng: -122.3421 },
      zoom: 10.4
    });

    var marker = new google.maps.Marker({
      position: { lat: 47.66982, lng: -122.38476 },
      map: map,
      title: 'Ballard library!',
      markerLabel: "This is my marker label"
    });
    var marker = new google.maps.Marker({
      position: { lat: 47.65028, lng: -122.36156 },
      map: map,
      title: '!',
      markerLabel: "This is my marker label"
    });
